{
    "manifest_version": 3,
    "name": "ДВФУ: Где я в списке?",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.dvfu.ru/admission/spd/"],
            "js": ["highlighter/highlighter.js"],
            "css": ["highlighter/highlighter.css"]
        }
    ],

    "action": {
        "default_popup": "interface/index.html"
    },

    "permissions": [
        "storage"
    ],

    "icons": {
        "16": "favicons/icon-16.png",
        "48": "favicons/icon-48.png",
        "128": "favicons/icon-128.png"
    }
}